@prefix v: <http://view/>.
@prefix attr:  <http://view/dot/attribute/>.

<urn:ann> a v:Node ;
    attr:label "Ann" .

<urn:bern> a v:Node ;
    attr:shape "rectangle" ;
    attr:label "bern" .

[] a v:Edge ;
    v:source <urn:ann> ;
    v:target <urn:bern> ;
    attr:label "lives in" ;
    attr:tooltip "urn:livesIn" .