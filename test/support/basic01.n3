@prefix : <http://example/org/> .
@prefix v: <http://view/> .
@prefix attr:  <http://view/dot/attribute/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix earl: <http://www.w3.org/ns/earl#> .

:p1 a :Person ;
    :name "Ann";
    :livesIn _:b1 .

_:b1 :containedIn :uk ;
    :name "London"@en , "Londra"@it .

{

    ?p1 a v:Node ; v:term :p1 ; attr:label "p1" ; attr:tooltip "http://example/org/p1" .
    ?b1 a v:Node ; attr:shape "circle" ; attr:label "".

    [] a v:Edge ;
        v:source ?p1 ;
        v:target [ a v:Node ; v:term :Person ; attr:label "Person" ; attr:tooltip "http://example/org/Person" ] ;
        v:term rdf:type ;
        attr:label "type" ;
        attr:tooltip "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" .

    [] a v:Edge ;
        v:source ?p1 ;
        v:target [ a v:Node ; v:term "Ann" ; attr:label "Ann" ; attr:tooltip "http://www.w3.org/2001/XMLSchema#string" ] ;
        v:term :name ;
        attr:label "name" ;
        attr:tooltip "http://example/org/name" .

    [] a v:Edge ;
        v:source ?p1 ;
        v:target ?b1 ;
        v:term :livesIn ;
        attr:label "livesIn" ;
        attr:tooltip "http://example/org/livesIn" .

    [] a v:Edge ;
        v:source ?b1 ;
        v:target [ a v:Node ; v:term :uk ; attr:label "uk" ; attr:tooltip "http://example/org/uk" ] ;
        v:term :containedIn ;
        attr:label "containedIn" ;
        attr:tooltip "http://example/org/containedIn" .

    [] a v:Edge ;
        v:source ?b1 ;
        v:target [ a v:Node ; v:term "London"@en ; attr:label "London" ; attr:tooltip "en" ] ;
        v:term :name ;
        attr:label "name" ;
        attr:tooltip "http://example/org/name" .

    [] a v:Edge ;
        v:source ?b1 ;
        v:target [ a v:Node ; v:term "Londra"@it ; attr:label "Londra" ; attr:tooltip "it" ] ;
        v:term :name ;
        attr:label "name" ;
        attr:tooltip "http://example/org/name" .

} => { [] a earl:Pass } .