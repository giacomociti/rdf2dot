{
  "name": "rdf2dot-root",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "rdf2dot": "bin/rdf2dot.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "vitest run && vite build && npm run build:wcs && cp ./packages/rdf2dot/vocabulary.ttl ./dist/vocabulary.ttl",
    "build:wcs": "vite build --config packages/rdf2dot-wc/vite.config.js",
    "preview": "vite preview",
    "test": "vitest"
  },
  "devDependencies": {
    "approvals": "^6.2.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.4"
  },
  "dependencies": {
    "@rdfjs-elements/rdf-editor": "^0.5.10",
    "@viz-js/viz": "^3.1.0",
    "eyereasoner": "^12.15.0",
    "file-saver": "^2.0.5"
  },
  "workspaces": [
    "packages/rdf2dot",
    "packages/rdf2dot-wc"
  ]
}
