@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix v: <http://view/> .
@prefix attr: <http://view/dot/attribute/> .

{ ?s a <urn:City> ; log:localName ?name } => 
{ ?s a v:Node ; attr:label ?name ; attr:shape "rectangle" } .

{ ?s <urn:name> ?o . (?name xsd:string) log:dtlit ?o } =>
{ ?s a v:Node ; attr:label ?name } .

{ ?s <urn:livesIn> ?o . <urn:livesIn> log:uri ?tooltip } =>
{ [] a v:Edge ; v:source ?s ; v:target ?o ; attr:label "lives in" ; attr:tooltip ?tooltip } .